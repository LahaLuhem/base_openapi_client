{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Generate C# client",
      "type": "shell",
      "command": "${workspaceFolder}/Library/csharp_nswag-generator-cli.sh",
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "clear": true
      }
    },
    {
      "label": "Generate Dart client",
      "type": "shell",
      "command": "${workspaceFolder}/Library/dart_openapi-generator-cli.sh",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      }
    },
    {
      "label": "Generate Python client",
      "type": "shell",
      "command": "${workspaceFolder}/Library/python_openapi-generator-cli.sh",
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "clear": true
      }
    }
  ]
}